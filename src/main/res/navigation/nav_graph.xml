<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainActivity">

    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.electricalcalculator.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" >
        <action
            android:id="@+id/next"
            app:destination="@id/selector" />
    </activity>
    <activity
        android:id="@+id/selector"
        android:name="com.example.electricalcalculator.selector"
        android:label="activity_selector"
        tools:layout="@layout/activity_selector" />
</navigation>